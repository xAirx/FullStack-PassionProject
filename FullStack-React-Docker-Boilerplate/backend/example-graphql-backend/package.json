    "apollo-client-codegen": "^0.0.2",
    "apollo-server-express": "^2.6.3",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.11.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "mongodb": "^3.5.5",
    "object-hash": "^2.0.3",
    "pino": "^5.17.0",
    "pretty-bytes": "^5.3.0",
