  "description": "This demo is a minimal Node.js Typescript application providing a GraphQL API.",
  "main": "index.js",
  "scripts": {
    "generate": "gql-gen",
    "build": "tsc",
    "start": "node --max_old_space_size=128 --inspect ./build/index.js",
    "test": "npm run build && jest --clearCache && jest --verbose"
