    "@types/kafkajs": "^1.9.0",
    "@types/mongodb": "^3.5.2",
    "@types/node": "^13.9.2",
    "@types/object-hash": "^1.3.1",
    "@types/pg": "^7.14.3",
    "@types/pino": "^5.17.0",
    "@types/uuid": "^7.0.2",
    "@types/uuid-validate": "0.0.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "clear": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.1.0",
    "pino-pretty": "^3.6.1",
    "prettier": "^2.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@apollo/federation": "^0.13.2",
    "@babel/preset-env": "^7.9.0",
