    "sinon": "^9.0.1",
    "uglify-js": "^3.8.0",
    "uuid": "^7.0.2",
    "uuid-by-string": "^3.0.2",
    "uuid-validate": "0.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
